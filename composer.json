{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": "~7.2.0",
    "wp-cli/wp-cli": "~0.22",
    "wpackagist-plugin/amazon-s3-and-cloudfront": "~2.1",
    "wpackagist-plugin/insert-html-snippet": "~1.2.5",
    "wpackagist-plugin/akismet": "~4.1.0",
    "wpackagist-plugin/perfect-woocommerce-brands": "~1.7.0",
    "wpackagist-plugin/sendgrid-email-delivery-simplified": "~1.11.4",
    "wpackagist-plugin/woocommerce": "~3.5.2",
    "wpackagist-plugin/woocommerce-gateway-stripe": "~4.1.13",
    "wpackagist-theme/storefront": "~2.3.5"
  },
  "scripts": {
    "post-install-cmd": "wp plugin activate akismet sendgrid-email-delivery-simplified woocommerce insert-html-snippet perfect-woocommerce-brands woocommerce-gateway-stripe amazon-web-services amazon-s3-and-cloudfront; wp theme activate storefront-child",
    "test": "phpunit -v tests"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5.0"
  }
}
